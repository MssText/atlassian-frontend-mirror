    "@atlaskit/range": "^6.1.0",
    "@atlaskit/spinner": "^15.0.0",
    "@atlaskit/theme": "^12.2.0",
    "@atlaskit/tooltip": "^17.6.0",
    "@babel/runtime": "^7.0.0",
